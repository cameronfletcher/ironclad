{
  "urls": "http://*:80;http://*:5005;",
  "serilog": {
    "minimumLevel": {
      "default": "Information"
    },
    "writeTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "RollingFile",
              "Args": { "pathFormat": "logs/ironclad/ironclad-docker-{Date}.log" }
            }
          ]
        }
      }
    ]
  },
  "connectionStrings": {
    "Ironclad": "Host=postgres;Database=ironclad;Username=username;Password=password;"
  }
}